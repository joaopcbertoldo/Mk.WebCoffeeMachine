<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <appSettings>
    <add key="server.name" value="MKafeína" />
    <!-- USP - cabled network - public IP -->
    <!--add key="serverAddress" value="http://143.107.226.186:80" / -->
    <!-- Home network -->
    <add key="server.address" value="http://192.168.0.3:80" />
    <add key="server.niceAddress" value="http://www.mkafeina.eesc.usp.br" />
    
    <!-- PANELS CONFIGS *************************************** -->
    <add key="panelsNames" value="status, configs, commands, log" />

    <!-- STATUS PANEL CONFIGS ******************************* -->
    <add key="status.title" value="STATUS" />
    <add key="status.width" value="fullWindow" />
    <add key="status.height" value="6" />
    <add key="status.columns" value="2" />
    <add key="status.lines" value="server.address,server.niceAddress" />

    <!-- CONFIGS PANEL CONFIGS ******************************* -->
    <add key="configs.title" value="CONFIGURATIONS" />
    <add key="configs.width" value="fullWindow" />
    <add key="configs.height" value="5" />
    <add key="configs.columns" value="3" />
    <add key="configs.lines" value="" />

    <!-- COMMANDS PANEL CONFIGS ******************************* -->
    <add key="commands.title" value="COMMANDS" />
    <add key="commands.width" value="fullWindow" />
    <add key="commands.height" value="2" />
    <add key="commands.columns" value="3" />
    <add key="commands.lines" value="F4,F5" />
    
    <!-- LOG PANEL CONFIGS ******************************* -->
    <add key="log.title" value="LOG" />
    <add key="log.width" value="fullWindow" />
    <add key="log.height" value="15" />
    <add key="log.columns" value="1" />
    <add key="log.lines" value="" />
    
    <!-- COOK BOOK *************************************** -->
    <add key="recipes" value="expresso, double, aguado" />
    <add key="ingredients" value="c, w" />

    <add key="expresso.c" value="5" />
    <add key="expresso.w" value="50" />

    <add key="double.c" value="10" />
    <add key="double.w" value="50" />

    <add key="aguado.c" value="5" />
    <add key="aguado.w" value="150" />
    
  </appSettings>
